{
  "name": "commit-message-generator",
  "displayName": "AI Commit Message Generator",
  "description": "Generate short and informative commit messages using AI",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:commit-message-generator.generateMessage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "commit-message-generator.generateMessage",
        "title": "Generate Commit Message"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.6.0",
    "openai": "^4.0.0"
  }
}
